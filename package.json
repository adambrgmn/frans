{
  "name": "frans",
  "version": "0.0.0-semantically-released",
  "description": "A minimal functional programming library",
  "main": "dist/frans.cjs.js",
  "jsnext:main": "dist/frans.esm.js",
  "module": "dist/frans.esm.js",
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "https://github.com/adambrgmn/frans.git"
  },
  "author": "Adam Bergman <adam@fransvilhelm.com>",
  "license": "MIT",
  "scripts": {
    "build": "frans-scripts bundle",
    "test": "frans-scripts test",
    "lint": "frans-scripts lint",
    "format": "frans-scripts format",
    "precommit": "frans-scripts precommit",
    "validate": "yarn run lint && yarn run test --coverage && yarn run build",
    "release": "frans-scripts release",
    "create-module": "node scripts/create-module.js",
    "bench": "frans-scripts build --quiet && node scripts/benchmark.js"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "cz-conventional-changelog": "^2.1.0",
    "frans-scripts": "^2.1.3",
    "ora": "^2.0.0"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
